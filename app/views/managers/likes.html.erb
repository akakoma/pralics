<div class="container">
  <div class="managers-info-wrapper">
    <h1>お気に入り情報</h1>
    <h2>お気に入り総数 : <%= @likes.length %></h2>

    <select class="" name="">
      <option value="">id</option>
      <option value="">event_id</option>
      <option value="">user_id</option>
      <option value="">created_at</option>
      <option value="">updated_at</option>
    </select>
    <input type="serch" name="" value="">
    <div class="managers-each-info">
      <h3>All</h3>
      <table border="1" class="managers-info-content">
        <tr>
          <th>id</th>
          <th>event_id</th>
          <th>user_id</th>
          <th>created_at</th>
          <th>updated_at</th>
          <th>編集</th>
        </tr>

        <% @likes.each do |like| %>
        <tr>
          <td><%= like.id %></td>
          <td><%= like.event_id %></td>
          <td><%= like.user_id %></td>
          <td><%= like.created_at %></td>
          <td><%= like.updated_at %></td>
          <td><%= link_to "編集","/managers/likes/#{like.id}/edit"%></td>
        </tr>
        <% end %>
      </table>
    </div>

  </div>
</div>
