  <div class=" content active" id="user">
    <div class="new-wrapper clearfix">
      <h1>User</h1>
    <%= form_tag('/managers/users/create',class:"user-new-wrapper") do %>
    <h2>name</h2>
    <input type="text" name="name" value="<%= @user.name %>">
    <h2>user_id</h2>
    <input type="text" name="user_id" value="<%= @user.user_id %>">
    <h2>email</h2>
    <input type="text" name="email" value="<%= @user.email %>">
    <h2>age</h2>
    <select class="" name="age">
      <% for i in 15..100 %>
      <option value="<%= i %>"><%= i %></option>
      <% end %>
    </select>
    <h2>sex</h2>
    <div class="new-sex">
      <input type="radio" name="sex" value="Man">男性<br>
      <input type="radio" name="sex" value="Woman">女性<br>
      <input type="radio" name="sex" value="LGBT">LGBT<br>
    </div>
    <h2>password</h2>
    <input type="text" name="password" value="<%= @user.password %>">
    <h2>organizer_id</h2>
    <input type="text" name="organizer_id" value="<%= @user.organizer_id %>">
    <input type="submit" value="作成">
    <% end %>
  </div>
  </div>

