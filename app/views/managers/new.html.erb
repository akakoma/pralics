<div class="container">
  <div class="wrapper-type-a">
    <h1>新規登録</h1>
  <ul class="manager-new-menu">
    <li class="menu-item choose" data-id='user'>User</li>
    <li class="menu-item" data-id='group'>Group</li>
    <li class="menu-item" data-id='company'>Comany</li>
    <li class="menu-item" data-id='event'>Event</li>
    <li class="menu-item" data-id='estaevent'>Estaevent</li>
    <li class="menu-item" data-id='manager'>Manager</li>
    <li class="menu-item" data-id='attend'>Attend</li>
    <li class="menu-item" data-id='like'>Like</li>
    <li class="menu-item" data-id='usersubgroup'>Usersubgroup</li>
