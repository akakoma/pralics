    <h2 style="color:#333;">email</h2>
    <input type="text" name="email" value="<%= @company.email %>">
    <h2 style="color:#333;">password</h2>
    <input type="text" name="password" value="<%= @company.password %>">
    <h2 style="color:#333;">manager</h2>
    <input type="text" name="manager" value="<%= @company.manager %>">
    <h2 style="color:#333;">tel</h2>
    <input type="text" name="tel" value="<%= @company.tel %>">
    <h2 style="color:#333;">image</h2>
    <input type="file" name="image" value="">
    <input type="submit" name="" value="作成">
    <% end %>
