<div class="container">
  <div class="managers-info-wrapper">
    <h1>マネージャー情報</h1>
    <h2>マネージャー総数 : <%= @managers.length %></h2>

    <select class="" name="">
      <option value="">id</option>
      <option value="">name</option>
      <option value="">password</option>
      <option value="">created_at</option>
      <option value="">updated_at</option>
    </select>
    <input type="serch" name="" value="">
    <div class="managers-each-info">
      <h3>All</h3>
      <table border="1" class="managers-info-content">
        <tr>
          <th>id</th>
          <th>name</th>
          <th>password_digest</th>
          <th>created_at</th>
          <th>updated_at</th>
          <th>編集</th>
        </tr>

        <% @managers.each do |manager| %>
        <tr>
          <td><%= manager.id %></td>
          <td><%= manager.name %></td>
          <td><%= manager.password_digest %></td>
          <td><%= manager.created_at %></td>
          <td><%= manager.updated_at %></td>
          <td><%= link_to "編集","/managers/managers/#{manager.id}/edit"%></td>
        </tr>
        <% end %>
      </table>
    </div>

  </div>
</div>
