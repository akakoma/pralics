<div class="container">
  <div class="managers-info-wrapper">
    <h1 style="padding-left:10px;">ユーザー団体加入編集画面</h1>
      <h2 style="padding-left:10px;">ユーザー団体加入ID: <%= @usersubgroup.id %></h2>
      <h2 style="padding-left:10px;">created_at : <%= @usersubgroup.created_at %></h2>
      <h2 style="padding-left:10px;">updated_at : <%= @usersubgroup.updated_at %></h2>
      <%= form_tag("/managers/usersubgroups/#{@usersubgroup.id}/update",method: :post,class: "edit-wrapper") do %>
        <h2>group_id</h2>
        <input type="text" name="group_id" value="<%= @usersubgroup.group_id %>">
        <h2>user_id</h2>
        <input type="text" name="user_id" value="<%= @usersubgroup.user_id %>">
        <h2>group_info</h2>
        <input type="text" name="group_info" value="<%= @usersubgroup.group_info %>">
        <input type="submit" value="編集">
      <%end%>
        <%= link_to "削除","/managers/usersubgroups/#{@usersubgroup.id}/destroy",method: :post,id: "edit-destroy" %>
  </div>
</div>
